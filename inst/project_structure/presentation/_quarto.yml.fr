project:
  type: default
  output-dir: outputs

format:
  revealjs:
    theme: [default, custom.scss]
    slide-number: true
    transition: slide
    background-transition: fade
    code-fold: false
    code-tools: true
    code-link: true
    embed-resources: true

execute:
  freeze: auto
  cache: true
  warning: false
  message: false
