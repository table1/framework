project:
  type: default
  output-dir: results/public

format:
  html:
    theme: cosmo
    toc: true
    toc-depth: 3
    code-fold: false
    code-tools: true
    code-link: true
    df-print: paged
    embed-resources: false

execute:
  freeze: auto
  cache: true
  warning: false
  message: false
