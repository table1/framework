> ##D db_transaction(conn, {
> ##D   DBI::dbExecute(conn, "INSERT INTO users (name, age) VALUES ('Alice', 30)")
> ##D   DBI::dbExecute(conn, "INSERT INTO users (name, age) VALUES ('Bob', 25)")
> ##D })
> ##D 
> ##D # Transaction with error handling - auto-rollback on error
> ##D tryCatch({
> ##D   db_transaction(conn, {
> ##D     DBI::dbExecute(conn, "INSERT INTO users (name) VALUES ('Alice')")
> ##D     stop("Something went wrong")  # This will trigger rollback
> ##D   })
> ##D }, error = function(e) {
> ##D   message("Transaction failed: ", e$message)
> ##D })
> ##D 
> ##D DBI::dbDisconnect(conn)
> ## End(Not run)
